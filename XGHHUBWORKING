-- Scripts

local function NEEW_fake_script() -- OpenUI.Script 
	local script = Instance.new('Script', OpenUI)

	local OB = script.Parent
	local Main = OB.Parent.MAIN
	
	OB.MouseButton1Click:Connect(function()
		Main.Visible = true
	end)
end
coroutine.wrap(NEEW_fake_script)()
local function LYCNZ_fake_script() -- InfYeild.Script 
	local script = Instance.new('Script', InfYeild)

	local InfYeild = script.Parent
	
	InfYeild.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
	end)
end
coroutine.wrap(LYCNZ_fake_script)()
local function GCIGCGA_fake_script() -- DestroyButton.Script 
	local script = Instance.new('Script', DestroyButton)

	local DB = script.Parent
	local main = DB.Parent.Parent
	
	DB.MouseButton1Click:Connect(function()
		main:Destroy()
	end)
end
coroutine.wrap(GCIGCGA_fake_script)()
